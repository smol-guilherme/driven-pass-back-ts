// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// model _ {
//   id Int @id @default(autoincrement())
// }

model Users {
  id Int @id @default(autoincrement())
  email String @unique 
  password String 
}

model Credentials {
  id Int @id @default(autoincrement())
  url String 
  title String @unique 
  username String 
  password String 
}
